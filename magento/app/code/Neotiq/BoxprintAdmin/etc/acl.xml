<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Neotiq_Neotiq::neotiq" title="Neotiq : Role" sortOrder="10">
                    <resource id="Neotiq_BoxprintAdmin::boxprint" title="Neotiq:Boxo Print" sortOrder="10">
                        <resource id="Neotiq_BoxprintAdmin::template" title="Neotiq:Template" sortOrder="20" />
                        <resource id="Neotiq_BoxprintAdmin::workspace" title="Neotiq:Workspace" sortOrder="30" />
						<resource id="Neotiq_BoxprintAdmin::workspace_default" title="Neotiq:Workspace Dfault" sortOrder="35" />
                        <resource id="Neotiq_BoxprintAdmin::upload" title="Neotiq:Upload" sortOrder="50" />
						 <resource id="Neotiq_BoxprintAdmin::workspace_general" title="Neotiq:General Workspace" sortOrder="60" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Neotiq_BoxprintAdmin::neotiq_boxprint_config" title="Settings" sortOrder="80" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>

